<body style="background-image: url(/assets/Background.png);">

    <header>
    </header>

    <p class="text-center">
        <span class="gall">GALLERY</span>

    </p>


    <div class="nav2list-position">
        <ul class="nav2list">
            <li class="nav2">

                @Html.ActionLink("All", "Index", "GalleryCustomer", null, new { @class = "black-text" }) &nbsp;         |&nbsp;
            </li>
            <li class="nav2">
                @Html.ActionLink("SoloIto", "SoloIndex", "GalleryCustomer", null, new { @class = "black-text" }) &nbsp;      |&nbsp;
            </li>
            <li class="nav2">
                &nbsp;
                @Html.ActionLink("Family", "FamilyIndex", "GalleryCustomer", null, new { @class = "black-text" }) &nbsp;       |&nbsp;
            </li>
            <li class="nav2">
                @Html.ActionLink("Duo", "FurIndex", "GalleryCustomer", null, new { @class = "black-text", @style = "color: #d18d06" }) &nbsp;      |&nbsp;

            </li>
            <li class="nav2">
                @Html.ActionLink("Barkada", "CoupleIndex", "GalleryCustomer", null, new { @class = "black-text" }) &nbsp;

            </li>

        </ul>


    </div>



    <div class="container">
        @{
            if (Model.GalleryFiles.Count == 0)
            {
                <text>
                    <p class="noitems"> No items found </p>
                </text>
            }
            else
            {
                for (int i = 0; i < Model.GalleryFiles.Count; i++)
                {
                    <div class="responsive">
                        <div class="gallery">
                            <a target="_blank">
                                <img src="@System.IO.Path.Combine("../uploads", Model.GalleryFiles[i].fileName)" alt="Forest" width="600" height="400">
                            </a>
                        </div>
                    </div>
                }
            }
        }
    </div>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.3.1.js"
            asp-fallback-src="~/lib/jquery/dist/jquery.js"
            asp-fallback-test="window.jQuery"
            crossorigin="anonymous"
            integrity="sha384-tsQFqpEReu7ZLhBV2VZlAu7zcOV+rXbYlF2cqB8txI/8aZajjp4Bqd+V6D5IgvKT">
    </script>



    <script src="https://cdjns.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

