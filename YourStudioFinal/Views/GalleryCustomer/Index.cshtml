                </nav>

                <div class="wrapper-navbar-logo">

                    <img src="~/assets/logo.png" alt="Your Studio Logo">
                    <img class="logonav2" src="~/assets/logo2.png" alt="Your Studio Logo">
                </div>
            </div>
        </header>

    <p class="text-center">
        <span class="gall">GALLERY</span>

    </p>

    <div class="nav2list-position">
        <ul class="nav2list">
            <li class="nav2">

                All &nbsp;         |
            </li>
            <li class="nav2">
                Solo &nbsp;      |
            </li>
            <li class="nav2">
                Family &nbsp;       |
            </li>
            <li class="nav2">
                Fur Families &nbsp;      |

            </li>
            <li class="nav2">
                Couples &nbsp;      |

            </li>
            <li class="nav2">
                Child

            </li>
        </ul>


    </div>



    <div class="container">
        @{
            if (Model.GalleryFiles.Count == 0)
            {
                <text>
                    <p class="noitems"> No items found </p>
                </text>
            }
            else
            {
                for (int i = 0; i < Model.GalleryFiles.Count; i++)
                {
                    <div class="responsive">
                        <div class="gallery">
                            <a target="_blank">
                                <img src="@System.IO.Path.Combine("uploads", Model.GalleryFiles[i].fileName)" alt="Forest" width="600" height="400">
                            </a>
                        </div>
                    </div>
                }
            }
        }
    </div>
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.3.1.js"
